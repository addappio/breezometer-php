{
    "name": "addapp/breezometer-php",
    "description": "PHP client package for the Breezometer",
    "keywords": [
        "php",
        "Breezometer",
        "Air",
        "Pollution"
    ],
    "homepage": "https://github.com/addapp/Breezometer",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Widart",
            "email": "n.widart@gmail.com",
            "homepage": "http://www.nicolaswidart.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "guzzlehttp/guzzle": "5.2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Addapp\\Breezometer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Addapp\\Breezometer\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
